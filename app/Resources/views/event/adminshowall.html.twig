{# views/event/showall.html.twig #}

{% extends 'user.html.twig' %}

{% block stylesheets %}
      {{ parent() }}
   <link href="{{ asset('/css/event/showall.css') }}" rel="stylesheet" />
{% endblock %}

{% block myheader %} {{ parent() }}{% endblock %}


{% block middle %}
  <div id="middle" >
   {% if message %}
     <h1> {{message}}</h1>
   {% endif %}

   {% if events %}
     <h1>{{heading}}</h1> 
     <nav>
       <ul>
         {% for event in events %}
            <li><a href="{{event.link}}" >{{ event.label}}</a> </li>
         {% endfor %}
      </ul>
     </nav>
  {% endif %}
  </div>
{% endblock %}
